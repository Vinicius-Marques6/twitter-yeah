{
  "manifest_version": 3,
  "name": "Twitter Yeah!",
  "description": "Yeah!!",
  "version": "1.1",
  "action": {
    "default_popup": "hello.html"
  },
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": ["https://x.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/yeah.js",
        "scripts/jquery-3.6.4.min.js"
      ],
      "matches": [
        "https://x.com/*"
      ]
    }
  ]
}